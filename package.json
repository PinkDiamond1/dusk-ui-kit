{
  "name": "dusk-ui-kit",
  "private": true,
  "description": "Dusk UI Kit",
  "homepage": "https://dusk-network.github.io/dusk-ui-kit/",
  "bugs": {
    "url": "https://github.com/dusk-network/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dusk-network/dusk-ui-kit.git"
  },
  "license": "UNLICENSED",
  "author": "Kieran Hall <kieran@dusk.network>",
  "files": [],
  "workspaces": [
    "packages/**"
  ],
  "scripts": {
    "format": "eslint . --ignore-pattern 'node_modules/' --ignore-pattern 'storybook-static/' --fix && prettier . --write --plugin-search-dir=. \"{,!(node_modules,storybook-static)/**/**/}*.{json,js,svelte}\"",
    "sb:build": "build-storybook",
    "storybook": "start-storybook -p 6006",
    "test": "run-p test:*",
    "test:lint": "eslint . --ignore-pattern 'node_modules/' --ignore-pattern 'storybook-static/'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true && npx --no-install devmoji -e --lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "post-commit": "git update-index --again",
      "commit": "npx git-cz"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-lerna-changelog"
    }
  },
  "dependencies": {
    "eslint-plugin-cypress": "^2.10.3"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.0.6",
    "@semantic-release/npm": "^7.0.5",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@storybook/addon-a11y": "^5.3.13",
    "@storybook/addon-actions": "^5.3.12",
    "@storybook/addon-backgrounds": "^5.3.13",
    "@storybook/addon-centered": "^5.3.13",
    "@storybook/addon-docs": "^5.3.13",
    "@storybook/addon-knobs": "^5.3.13",
    "@storybook/addon-links": "^5.3.12",
    "@storybook/addon-notes": "^5.3.13",
    "@storybook/addon-storysource": "^5.3.13",
    "@storybook/addon-viewport": "^5.3.13",
    "@storybook/addons": "^5.3.12",
    "@storybook/source-loader": "^5.3.13",
    "@storybook/svelte": "^5.3.12",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/svelte": "^3.0.0",
    "babel-loader": "^8.0.6",
    "commitizen": "^4.1.2",
    "cz-lerna-changelog": "^2.0.2",
    "devmoji": "^2.1.9",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-svelte3": "^2.7.3",
    "husky": "^4.2.5",
    "lerna": "^3.4.3",
    "lint-staged": "^10.2.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.26",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.2.1",
    "postcss-url": "^8.0.0",
    "prettier": "^1.19.1",
    "prettier-plugin-packagejson": "^2.2.3",
    "prettier-plugin-svelte": "^1.1.0",
    "semantic-release": "^17.0.7",
    "semantic-release-monorepo": "^7.0.2",
    "semantic-release-monorepo-hooks": "^2.9.5",
    "semantic-release-plugin-decorators": "^3.0.0",
    "stylelint": "^13.2.0",
    "stylelint-config-standard": "^20.0.0",
    "svelte": "^3.0.0",
    "svelte-loader": "^2.13.6",
    "svelte-preprocess": "^3.4.0",
    "tailwindcss": "^1.2.0"
  },
  "publishConfig": {
    "registry": "https://npm.dusk.network:4873"
  },
  "release": {
    "extends": "semantic-release-monorepo",
    "branches": [
      "master",
      "next"
    ]
  }
}
