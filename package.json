{
  "name": "dusk-ui-kit",
  "private": true,
  "description": "Dusk UI Kit",
  "homepage": "https://dusk-network.github.io/dusk-ui-kit/",
  "bugs": {
    "url": "https://github.com/dusk-network/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dusk-network/dusk-ui-kit.git"
  },
  "license": "UNLICENSED",
  "author": "Kieran Hall <kieran@dusk.network>",
  "files": [],
  "workspaces": [
    "packages/**"
  ],
  "scripts": {
    "lint": "prettier --write --plugin-search-dir=. ./packages/**/* && stylelint \"**/*.css\" --fix",
    "sb:build": "build-storybook",
    "semantic-release": "semantic-release",
    "storybook": "start-storybook -p 6006",
    "test": "echo \"LOL\" && exit 1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true && npx --no-install devmoji -e --lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "post-commit": "git update-index --again",
      "pre-push": "yarn test",
      "commit": "npx git-cz"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@storybook/addon-a11y": "^5.3.13",
    "@storybook/addon-actions": "^5.3.12",
    "@storybook/addon-backgrounds": "^5.3.13",
    "@storybook/addon-centered": "^5.3.13",
    "@storybook/addon-docs": "^5.3.13",
    "@storybook/addon-knobs": "^5.3.13",
    "@storybook/addon-links": "^5.3.12",
    "@storybook/addon-notes": "^5.3.13",
    "@storybook/addon-storysource": "^5.3.13",
    "@storybook/addon-viewport": "^5.3.13",
    "@storybook/addons": "^5.3.12",
    "@storybook/source-loader": "^5.3.13",
    "@storybook/svelte": "^5.3.12",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/svelte": "^3.0.0",
    "babel-loader": "^8.0.6",
    "cz-conventional-changelog": "3.2.0",
    "devmoji": "^2.1.9",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "jest-transform-svelte": "^2.1.1",
    "lerna": "^3.20.2",
    "lint-staged": "^10.2.2",
    "nightwatch": "^1.3.4",
    "postcss": "^7.0.26",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.2.1",
    "postcss-url": "^8.0.0",
    "prettier": "^1.19.1",
    "prettier-plugin-packagejson": "^2.2.3",
    "prettier-plugin-svelte": "^0.7.0",
    "semantic-release": "^17.0.7",
    "stylelint": "^13.2.0",
    "stylelint-config-standard": "^20.0.0",
    "svelte": "^3.0.0",
    "svelte-loader": "^2.13.6",
    "svelte-preprocess": "^3.4.0",
    "tailwindcss": "^1.2.0"
  },
  "publishConfig": {
    "registry": "https://npm.dusk.network:4873"
  }
}
